{
  "name": "stylobot.website",
  "version": "1.0.0",
  "description": "Stylobot.Website frontend",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:tw": "npx tailwindcss -i ./wwwroot/src/index.css -o ./wwwroot/dist/assets/index.css",
    "dev:js": "vite build --watch",
    "build": "npm-run-all build:*",
    "build:tw": "npx tailwindcss -i ./wwwroot/src/index.css -o ./wwwroot/dist/assets/index.css --minify",
    "build:js": "vite build",
    "watch": "npm-run-all --parallel watch:*",
    "watch:tw": "npx tailwindcss -i ./wwwroot/src/index.css -o ./wwwroot/dist/assets/index.css --watch",
    "watch:js": "vite build --watch",
    "preview": "vite preview --port 5173",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "autoprefixer": "^10.4.22",
    "daisyui": "^5.5.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "vite": "^7.2.7"
  },
  "dependencies": {
    "@alpinejs/csp": "^3.15.2",
    "alpinejs": "^3.15.2",
    "apexcharts": "^5.5.0",
    "htmx.org": "^2.0.8"
  }
}
