{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Mostlylucid.GeoDetection": "Debug"
    }
  },
  "AllowedHosts": "*",
  "GeoLite2": {
    "Provider": "IpApi",
    "AccountId": null,
    "LicenseKey": null,
    "DatabasePath": "data/GeoLite2-City.mmdb",
    "EnableAutoUpdate": true,
    "DownloadOnStartup": true,
    "DatabaseType": "City",
    "CacheDuration": "01:00:00",
    "FallbackToSimple": true
  }
}
